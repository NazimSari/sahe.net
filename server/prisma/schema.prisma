

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl =  env("DIRECT_URL")
}

model PreRegister {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  name      String
  talent    String
  createdAt DateTime @default(now())
}

model Counter {
  id        Int      @id @default(1) 
  value     Int      @default(0)     
  updatedAt DateTime @updatedAt      
}
